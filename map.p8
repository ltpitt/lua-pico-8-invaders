pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

-- initialize camera coordinates to the upper left corner of the camera
cam_y = 0
cam_x = 0
counter = 0

function wait()
print('1', 64, 60,7)
print('2', 64, 60,7)
print('3', 64, 60,7)
end
function _update()
if (btn(2) and cam_y > -376) cam_y -= 1
end

function _draw()
  cls(1)
  -- set the camera to the current location
  camera(cam_x,cam_y)

max_y_cells=128

mapdraw(0, -16, 120, -510, 32, max_y_cells)
mapdraw(0, -16, 0, -510, 32, max_y_cells)
for border=0,128 do
 mset(0,border,23)
 counter = counter +1
 if counter == 380 then cam_y = 0
 wait()
end
end
 
 
  -- reset the camera then print the camera
  -- coordinates on screen
  --camera()
  --print('('..cam_y..')', 0, 0, 7)
end

